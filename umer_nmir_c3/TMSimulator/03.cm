void func(int g, int x, int a){
  output(g);
  output(1000);
  output(1001);
  output(g + x * a);
  return; 
}

void main(void) {
  int x; int fac;

  x = input();
  fac = 1;

  while (x > 1) {
    fac = fac * x;
    x = x - 1;
  } 
  /*output(fac);*/
  func(fac, 1, 2);
  output(x);
  return;
  x = 0;
  output(x);
  
}
